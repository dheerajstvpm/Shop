                <a href="/product/{{lastRow2._id}}"><img src="{{lastRow2.image}}" style="width: 15rem; height: 15rem" class=" card-img-top" alt="..."></a>
                <div class="card-body">
                    <a href="/addToCart/{{lastRow2._id}}" class="btn btn-outline-dark">Add to cart</a>
                    <a href="/addToWishlist/{{lastRow2._id}}" class="btn btn-dark">Wishlist</a>
                </div>
            </div>
        </div>

        <div class="col">
            <div class="card" style="width: 15rem; height: 20rem">
                <a href="/product/{{lastRow3._id}}"><img src="{{lastRow3.image}}" style="width: 15rem; height: 15rem" class=" card-img-top" alt="..."></a>
                <div class="card-body">
                    <a href="/addToCart/{{lastRow3._id}}" class="btn btn-outline-dark">Add to cart</a>
                    <a href="/addToWishlist/{{lastRow3._id}}" class="btn btn-dark">Wishlist</a>
                </div>
            </div>
        </div>

        <div class="col">
            <div class="card" style="width: 15rem; height: 20rem">
                <a href="/product/{{lastRow4._id}}"><img src="{{lastRow4.image}}" style="width: 15rem; height: 15rem" class=" card-img-top" alt="..."></a>
                <div class="card-body">
                    <a href="/addToCart/{{lastRow4._id}}" class="btn btn-outline-dark">Add to cart</a>
                    <a href="/addToWishlist/{{lastRow4._id}}" class="btn btn-dark">Wishlist</a>
                </div>
            </div>
        </div>
    </div>

    {{/each}}
</div>